# Set in stage2:
# ---------------------------------------------------------
# set -ef +am
# isFirstRun
# VAR_*
# All functions in /start/functions

if [ -n "$VAR_QGIS_OPTIONS_PATH" ]
then
   VAR_CONFIG_DIR="$VAR_QGIS_OPTIONS_PATH"
fi
